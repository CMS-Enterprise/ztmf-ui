# Environment configuration for local development
# Copy this file to .env.development.local and fill in the values

# Backend API proxy configuration
# Default: http://localhost:3000 (matches makefile dev setup in backend repo)
VITE_CF_DOMAIN=http://localhost:3000

# Authentication token for local development
# Generate this token by following the frontend test login process
# The same token used to authenticate with the frontend can be used here
VITE_AUTH_TOKEN3=your-jwt-token-here

# Disable identity provider for local development
VITE_IDP_ENABLED=false

# Legacy proxy configuration (leave empty for default setup)
TOKEN=
PROXY_URI=